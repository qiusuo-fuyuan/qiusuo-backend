type Channel {
    id: ID!
    name: String!
    active: Boolean!
    channelType: ChannelType
    parentCommunity: Community
}

enum ChannelType {
    QA
    CHAT
    LIVE_STREAMING
}

input CreateChannelInput {
    communityId: ID!
    name: String!
    channelType: ChannelType!
}


type ChannelMutation extend type Mutation {
    test: String!
}