type Channel {
    id: ID!
    name: String!
    channelType: ChannelType
    parentCommunity: Community
}

enum ChannelType {
    QA
    CHAT
    LIVE_STREAMING
}

input CreateChannelInput {
    communityId: ID!
    name: String!
    channelType: ChannelType!
}

type ChannelQuery extend type Query {
    activeChannel: Channel!
}

type ChannelMutation extend type Mutation {
    test: String!
}